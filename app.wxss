/**app.wxss**/
  @font-face {
    font-family: 'CocaKaiti';
    src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

.sub-background-container {
  position: fixed;
  left: 0rpx;
  right: 0rpx;
  top: 0rpx;
}
.sub-background-img {
  width: 750rpx;
}


page{
  width: 100%;
  height: 100%;
}
.btn{
  display: block;
  border-radius: 40px;
  font-size: 15px;
  color: #F7F7FA;
  letter-spacing: 0.1px;
  background-image: linear-gradient(-180deg, #e6c997 0%, #e5bf89 100%);
}
.btn-pierced{
  display: block;
  border-radius: 40px;
  font-size: 15px;
  color: #F7F7FA;
  letter-spacing: 0.1px;
  border: 1px solid #FE97B7;
  background: rgba(255,255,255,0);
}